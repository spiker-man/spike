<script>
//contador de dias sin accidentes

meses=new Array("Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec")

diasSemana = new Array("Domingo","Lunes","Martes","Miércoles","Jueves","Viernes","Sábado");


function countup(yr,m,d){
today=new Date()
todayy=today.getYear()
todaym=today.getMonth()
todayd=today.getDate()
todaystring=meses[todaym]+" "+todayd+", "+todayy
paststring=meses[m-1]+" "+(d+1)+", "+yr
dif=(Math.round((Date.parse(todaystring)-Date.parse(paststring))/(24*60*60*1000))*1)
return dif;
}
//poner aqui la fecha del accidente en el formato año/mes/dia 

document.getElementById("DSAcc").innerHTML = countup(2014,12,21);

//cambiar mensaje de acuerdo al dia de la semana
function mens(){

if(diasSemana[today.getDay()]=="Lunes"){
mensaje="“La Seguridad comienza con el trabajo en equipo.”";}
else if(diasSemana[today.getDay()]=="Martes"){
mensaje="“Los records de Seguridad no suceden por accidente.”";}
else if(diasSemana[today.getDay()]=="Miércoles"){
mensaje=("La Seguridad puede distinguirte. La falta de Seguridad puede extinguirte");}
else if(diasSemana[today.getDay()]=="Jueves"){
mensaje="“El mejor dispositivo de seguridad es un trabajador seguro.”";}
else if(diasSemana[today.getDay()]=="Viernes"){
mensaje="“Accidentes grandes o chicos, Evitalos Todos.”";}
else if(diasSemana[f.getDay()]=="Sábado"){
mensaje="“No dejes los hábitos seguros en el trabajo, llévalos a casa.”";}
else if(diasSemana[today.getDay()]=="Domingo"){
mensaje="“Que es lo que vale la pena? – Seguridad Primero.”";}

return mensaje;
}
document.getElementById("mensDia").innerHTML = mens();

function saludo(){
if(dif>5)
saludo="Gracias a ti llevamos"
else
saludo="Llevamos"
return saludo;
}
document.getElementById("salInicial").innerHTML = saludo();

function record() {

nuevoRec=352
if((nuevoRec-dif)>=(-1)){
if((nuevoRec-dif)>10)
record="Record a romper "+nuevoRec+" dias";
if((nuevoRec-dif)<=10)
record="Faltan "+(nuevoRec-dif)+" Dias para romper el record";
if((nuevoRec-dif)==0)
record="Ya igualamos el record";
if((nuevoRec-dif)==(-1))
record="FELICIDADES !!! Rompimos el record";
}
else
record="Record a romper "+dif+" diass";
return record;
}
document.getElementById("rec").innerHTML = record();
</script>

